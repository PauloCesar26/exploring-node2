<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <title>Usuários Cadastrados</title>
</head>
<body>
  <h1>Lista de Usuários</h1>
  <ul>
    <% if(user && user.length > 0) { %>
       <% console.log(user) %>
      <% user.forEach(users => { %>
        <div>
          <!-- <form action="/manage-user/delete/<%= users.id %>" method="post"> -->
            <img src="<%= users.userImg %>" width="100" height="100">
            <%= users.nome %> - <%= users.email %>
            <!-- <button>Delete</button> -->
          <!-- </form> -->
        </div>
      <% }) %>
    <% } else { %>
      <li>Nenhum user encontrado</li>
    <% } %>
  </ul>
  <a href="/admin/home">Voltar</a>
</body>
</html>