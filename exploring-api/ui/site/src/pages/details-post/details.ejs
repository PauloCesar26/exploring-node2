<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <title>Usu√°rios Cadastrados</title>
</head>
<body>
  <h1><%= post.nome %></h1>

  <% content.forEach(c => { %>
    <% if (c.type_content === 'title') { %>
      <p><%= c.content %></p>
    <% } %>

    <% if (c.type_content === 'text') { %>
      <p><%= c.content %></p>
    <% } %>

    <% if (c.type_content === 'image') { %>
      <img src="<%= c.image %>" width="500" height="300">
    <% } %>
  <% }) %>

  <a href="/">Voltar</a>
</body>
</html>